{
  "name": "redux-playground",
  "version": "0.0.7",
  "description": "redux-playground",
  "scripts": {
    "test": "better-npm-run coverage",
    "test:watch": "better-npm-run mocha:test --reporter min --no-exit --watch --watch-extensions js,json",
    "build": "better-npm-run build:production",
    "start": "better-npm-run start",
    "start:dev": "better-npm-run start:dev"
  },
  "engines": {
    "node": ">=5.10.0",
    "npm": ">=3.8.3"
  },
  "betterScripts": {
    "coverage": {
      "command": "babel-node node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- src/**/*.spec.js",
      "env": {
        "BABEL_ENV": "test"
      }
    },
    "mocha:test": {
      "command": "mocha src/**/*.spec.js",
      "env": {
        "BABEL_ENV": "test"
      }
    },
    "start": {
      "command": "node build/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:dev": {
      "command": "node server/server.dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:production": {
      "command": "webpack --config webpack.config.js -p && cp ./src/app/index.html ./build && cp ./server/server.prod.js ./build/index.js",
      "env": {
        "BABEL_ENV": "production",
        "NODE_ENV": "production"
      }
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drathal/redux-playground.git"
  },
  "dependencies": {
    "axios": "^0.10.0",
    "express": "^4.13.4",
    "i18next": "^2.5.1",
    "i18next-browser-languagedetector": "^0.2.2",
    "i18next-xhr-backend": "^0.5.3",
    "js-beautify": "^1.6.2",
    "normalize.css": "^4.1.1",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-i18next": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "react-toolbox": "^0.16.2",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.2.0",
    "esformatter": "^0.9.3",
    "esformatter-dot-notation": "^1.3.1",
    "esformatter-jsx": "^5.0.0",
    "esformatter-literal-notation": "^1.0.1",
    "esformatter-quotes": "^1.0.3",
    "esformatter-spaced-lined-comment": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ignore-styles": "^1.2.0",
    "image-webpack-loader": "^1.7.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "pretty-error": "^2.0.0",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
