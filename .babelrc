{
    "env": {
        "development": {
            "presets": ["react", "es2015", "stage-0", "react-hmre"],
            "plugins": [
                "transform-decorators-legacy"
            ]
        },
        "test": {
            "presets": ["react", "es2015", "stage-0"],
            "plugins": [
                "transform-decorators-legacy"
            ]
        },
        "production": {
            "presets": ["react", "es2015", "stage-0"],
            "plugins": [
                "transform-decorators-legacy",
                "transform-react-constant-elements"
            ]
        }
    }
}
